# Server Configuration
PORT=3000

# Debug Mode (set to 'true' to enable debug logs, 'false' for production)
DEBUG=false

# Gemini API Configuration
# Get your API key from: https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Higgsfield API Configuration
# Get your API credentials from: https://platform.higgsfield.ai
HIGGSFIELD_API_KEY=your_higgsfield_api_key_uuid_here
HIGGSFIELD_SECRET=your_higgsfield_secret_here

# Node Environment (set to 'production' to disable dev endpoints)
NODE_ENV=development
